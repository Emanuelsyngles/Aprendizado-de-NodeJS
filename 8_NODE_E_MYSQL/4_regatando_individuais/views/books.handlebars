<h1>Livros disponíveis</h1>
<div class="books-container">
    <a href="/" class="add">Adicionar livro</a>
    {{#each books}}
        <div>
            #{{this.id}} -<span class="bold"> <a href="/books/{{this.id}}">{{this.title}}</span></a> - {{this.pageqty}} Páginas
        </div>
    {{/each}}
</div>